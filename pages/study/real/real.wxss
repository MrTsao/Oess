/* pages/tabbar/study/random/random.wxss */
@import "../../../common/lib/chapterlist.wxss";
.page-section {
  border-top: 0px solid #eee;
}

.item {
  display: flex;
  align-items: center;
}

.item-image {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50rpx;
  margin-left: 20rpx;
}

.item-image image {
  width: 35rpx;
  height: 35rpx;
  padding: 8rpx 8rpx;
}

.border-top {
  border-top: 1px solid #eee;
}

.item-content {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex: 8;
  font-size: 28rpx;
  padding: 20rpx 30rpx;
}

.item-content2 {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex: 8;
  font-size: 28rpx;
  padding: 10rpx 30rpx;
  align-items: center;
}

.splited {
  height: 20rpx;
  background-color: #f5f5f5;
  width: 100%;
}

.content-fixed-top {
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 266rpx;
}

.sv-content {
  position: fixed;
  top: 266rpx;
  left: 0rpx;
  width: 100%;
}
